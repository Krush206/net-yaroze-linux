+       ASM_GENERATE_INTERNAL_LABEL (buf, "LCL", rs6000_pic_labelno);
+       assemble_name (file, buf);
+       fprintf (file, "-");
+       ASM_GENERATE_INTERNAL_LABEL (buf, "LCF", rs6000_pic_labelno);
+       assemble_name (file, buf);
+       fprintf (file, ")(%s)\n", reg_names[30]);
+       asm_fprintf (file, "\t{cax|add} %s,%s,%s\n",
