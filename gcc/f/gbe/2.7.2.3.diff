- 	  || (memrefs_conflict_p (SIZE_FOR_MODE (mem), XEXP (mem, 0),
- 				  SIZE_FOR_MODE (x), XEXP (x, 0), 0)
- 	      && ! (MEM_IN_STRUCT_P (mem) && rtx_addr_varies_p (mem)
- 		    && GET_MODE (mem) != QImode
- 		    && ! MEM_IN_STRUCT_P (x) && ! rtx_addr_varies_p (x))
- 	      && ! (MEM_IN_STRUCT_P (x) && rtx_addr_varies_p (x)
- 		    && GET_MODE (x) != QImode
- 		    && ! MEM_IN_STRUCT_P (mem) && ! rtx_addr_varies_p (mem))));
